    <EmbeddedResource Include="PubSymcResource\After_MZokusei.class" />
    <EmbeddedResource Include="PubSymcResource\After_MZokuseiMapper.class" />
    <EmbeddedResource Include="PubSymcResource\Before_AttributeResource.class" />
    <EmbeddedResource Include="PubSymcResource\Before_AttributeServiceImpl.class" />
    <EmbeddedResource Include="PubSymcResource\Before_MZokusei.class" />
    <EmbeddedResource Include="PubSymcResource\Before_MZokuseiMapper.class" />
  </ItemGroup>



  <ItemGroup>
    <Import Include="kikuchi" />
    <Import Include="Npgsql" />
    <Import Include="System.Data" />
    <Import Include="System.Drawing" />
    <Import Include="System.Windows.Forms" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Core" Version="1.50.0" />
    <PackageReference Include="BCrypt.Net-Next" Version="4.0.3" />
    <PackageReference Include="CXuesong.Shims.VisualBasic.Devices" Version="0.3.0" />
    <PackageReference Include="log4net" Version="3.2.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="10.0.102">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.NETCore.Platforms" Version="8.0.0-preview.7.23375.6" />
