<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows10.0.18362.0</TargetFramework>
    <StartupObject>Sub Main</StartupObject>
    <UseWindowsForms>true</UseWindowsForms>
    <MyType>WindowsForms</MyType>
    <ApplicationHighDpiMode>SystemAware</ApplicationHighDpiMode>
	<ForceDesignerDpiUnaware>true</ForceDesignerDpiUnaware>
    <ImportedNamespaces>kikuchi=False,Npgsql=False,System.Data=False,System.Drawing=False,System.Windows.Forms=False,Microsoft.VisualBasic=True,System=True,System.Collections=True,System.Collections.Generic=True,System.Diagnostics=True,System.Linq=True,System.Xml.Linq=True,System.Threading.Tasks=True,TeCASettings=True</ImportedNamespaces>
    <ApplicationIcon>Icons\64.ico</ApplicationIcon>
	<Version>3.3.1</Version>
	<InformationalVersion>3.00</InformationalVersion>
	<Company>Photron</Company>
    <PackageIcon>orgext.png</PackageIcon>
    <Title>TeCASettings</Title>
    <Authors>M.Kikuchi</Authors>
    <Product>TeCAシステム設定ツール</Product>
    <Configurations>Debug;Release;NewRelease;NewRelease2;LocalTest</Configurations>
    <Platforms>AnyCPU;x64</Platforms>
    <ApplicationHighDpiMode>SystemAware</ApplicationHighDpiMode>
    <ForceDesignerDpiUnaware>true</ForceDesignerDpiUnaware>
    <Copyright>Photron Limited.</Copyright>
    <FileVersion></FileVersion>
    <AssemblyVersion></AssemblyVersion>
    <NeutralLanguage>ja-JP</NeutralLanguage>
    <ApplicationManifest>My Project\app.manifest</ApplicationManifest>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
    <DefineDebug>False</DefineDebug>
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <DefineConstants>LOCALTEST</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <PlatformTarget>x64</PlatformTarget>
    <DefineDebug>False</DefineDebug>
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <DefineConstants>LOCALTEST</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='NewRelease2|AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
    <Optimize>True</Optimize>
    <DefineDebug>True</DefineDebug>
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <DefineConstants>LOCALTEST</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='NewRelease2|x64'">
    <PlatformTarget>x64</PlatformTarget>
    <Optimize>True</Optimize>
    <DefineDebug>True</DefineDebug>
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <DefineConstants>LOCALTEST</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
    <Optimize>False</Optimize>
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <DefineConstants>LOCALTEST</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='LocalTest|AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
    <Optimize>False</Optimize>
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <DefineConstants>LOCALTEST</DefineConstants>
    <DefineDebug>False</DefineDebug>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <PlatformTarget>x64</PlatformTarget>
    <Optimize>False</Optimize>
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <DefineConstants>LOCALTEST</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='LocalTest|x64'">
    <PlatformTarget>x64</PlatformTarget>
    <Optimize>False</Optimize>
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <DefineConstants>LOCALTEST</DefineConstants>
    <DefineDebug>False</DefineDebug>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='NewRelease|AnyCPU'">
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <PlatformTarget>x64</PlatformTarget>
    <DefineConstants>LOCALTEST</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='NewRelease|x64'">
    <OutputPath>$(OutputPath)$(TargetFramework.ToLowerInvariant())\</OutputPath>
    <DefineConstants>LOCALTEST</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="1.15.0.1Resources\**" />
    <EmbeddedResource Remove="1.15.0.1Resources\**" />
    <None Remove="1.15.0.1Resources\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Icons\64.ico" />
  </ItemGroup>

  <ItemGroup>
    <AdditionalFiles Remove="app.manifest" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="PubSymcResource\After_AttributeResource.class" />
    <None Remove="PubSymcResource\After_AttributeServiceImpl.class" />
    <None Remove="PubSymcResource\After_MZokusei.class" />
    <None Remove="PubSymcResource\After_MZokuseiMapper.class" />
    <None Remove="PubSymcResource\Before_AttributeResource.class" />
    <None Remove="PubSymcResource\Before_AttributeServiceImpl.class" />
    <None Remove="PubSymcResource\Before_MZokusei.class" />
    <None Remove="PubSymcResource\Before_MZokuseiMapper.class" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="PubSymcResource\After_AttributeResource.class" />
